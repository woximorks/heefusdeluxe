<p style="color: green"><%= notice %></p>

<h1>Upcoming shows</h1>

<div id="upcoming_shows">
  <% @upcoming_shows.each do |upcoming_show| %>
    <%= render upcoming_show %>
    <p>
      <%= link_to "Show this upcoming show", upcoming_show %>
    </p>
  <% end %>
</div>

<%= link_to "New upcoming show", new_upcoming_show_path %>
